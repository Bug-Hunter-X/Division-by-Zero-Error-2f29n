function add(a, b) {return a + b;} function sub(a, b) {return a - b;} function div(a, b) {if (b == 0) {return 0;} else {return a / b;}} function mul(a, b) {return a * b;} function mod(a, b) {if (b == 0) {return 0;} else {return a % b;}} let a = 10; let b = 0; console.log(add(a, b)); console.log(sub(a, b)); console.log(div(a, b)); console.log(mul(a, b)); console.log(mod(a, b));